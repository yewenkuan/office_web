<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.office_web.mapper.Sys_menuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.office_web.entity.Sys_menu">
        <id column="id" property="id" />
        <result column="menu_url" property="menu_url" />
        <result column="menu_parent_url" property="menu_parent_url" />
        <result column="menu_name" property="menu_name" />
    </resultMap>



    <select id="selectMenuList" resultType="com.example.office_web.mapper.Sys_menuMapper">
        select *  from sys_menu
    </select>
</mapper>
